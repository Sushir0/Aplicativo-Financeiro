<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finance"
        tools:targetApi="31" >
        <activity
            android:name=".lvl3.activitys.Configuracoes.Config_CategoriasActivity"
            android:exported="false"
            android:label="title_activity_config_categorias"
            android:theme="@style/Theme.Finance" />
        <activity
            android:name=".lvl3.activitys.Configuracoes.Config_MacroCategoriasActivity"
            android:exported="false"
            android:label="@string/title_activity_config_macro_categorias"
            android:theme="@style/Theme.Finance" />
        <activity
            android:name=".lvl3.activitys.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Finance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".lvl3.activitys.dashboardActivity"
            android:label="Dashboard"
            android:theme="@style/Theme.Finance" />
        <activity
            android:name=".lvl3.activitys.DetalhesParecelasActivity"
            android:label="Detalhes"
            android:theme="@style/Theme.Finance" />
        <activity android:name=".lvl3.activitys.ConfiguracaoActivity" />
        <activity android:name=".lvl3.activitys.GraficoActivity" />

    </application>

</manifest>